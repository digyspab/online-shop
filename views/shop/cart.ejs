    <body>
        <%- include('../includes/navigation.ejs') %>

        <main>
            <% if(products.length > 0) { %> 
                <ul>
                    <% products.forEach(data => { %> 
                        <li><%= data.productData.title %>(<%= data.qty %>)</li>
                    <% }); %> 
                </ul>
            <% } else { %> 
                <h1>Cart is Empty :-0</h1>
            <% } %> 
        </main>
        
